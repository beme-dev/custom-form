{
  "name": "@bemedev/custom-form",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/chlbri/custom-form"
  },
  "scripts": {
    "dev": "vite dev",
    "dev:host": "pnpm run dev --host",
    "_ci": "pnpm run config:off && pnpm run lint && pnpm run test",
    "ci": "pnpm run _ci && pnpm run p-q",
    "ci:admin": "pnpm run rm && pnpm run upgrade && pnpm run _ci && pnpm run p-q",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\" --log-level silent",
    "rinit": "pnpm run rm && pnpm run config",
    "rinit:off": "pnpm run rm && pnpm run config:off",
    "rm": "pnpx rimraf ./node_modules ./pnpm-lock.yaml",
    "test": "pnpm run test:coverage --no-coverage",
    "test:coverage": "vitest run",
    "build": "vite build",
    "config": "pnpm install",
    "config:off": "pnpm run config --offline",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "lint:watch": "onchange --await-write-finish 5500 -d 50000 -v \"**/*.ts\" -- pnpm run lint",
    "p-q": "pretty-quick",
    "start": "vite start",
    "pretest": "pnpm run build",
    "test:watch": "vitest",
    "before:e2e": "pnpm exec playwright install",
    "test:e2e": "pnpm run before:e2e && playwright test",
    "test:e2e:ui": "pnpm run before:e2e && playwright test --ui",
    "upgrade": "pnpm upgrade --latest",
    "prepare": "husky"
  },
  "dependencies": {
    "@ark-ui/solid": "^5.18.0",
    "@bemedev/app-solid": "^0.2.3",
    "@bemedev/app-ts": "^0.8.1",
    "@bemedev/pipe": "^0.0.9",
    "@bemedev/types": "^0.3.1",
    "@corvu/resizable": "^0.2.5",
    "@kobalte/core": "^0.13.10",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/solid-router": "^1.129.8",
    "@tanstack/solid-start": "^1.129.8",
    "@thisbeyond/solid-select": "^0.15.0",
    "@tiagotrindade/solid-colorful": "^1.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "color": "^5.0.0",
    "lucide-react": "^0.525.0",
    "redaxios": "^0.5.1",
    "solid-color": "^0.0.6",
    "solid-colorful": "^0.0.2",
    "solid-js": "^1.9.7",
    "solid-motionone": "^1.0.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.5",
    "vinxi": "0.5.8",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@bemedev/build-tests": "^0.1.1",
    "@bemedev/fsf": "^0.8.0",
    "@bemedev/sleep": "^0.1.2",
    "@bemedev/vitest-alias": "^0.0.3",
    "@bemedev/vitest-exclude": "^0.1.1",
    "@bemedev/vitest-extended": "^1.3.6",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@playwright/test": "^1.54.1",
    "@tanstack/solid-router-devtools": "^1.129.8",
    "@types/clone-deep": "^4.0.4",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "combinate": "^1.1.11",
    "eslint": "^9.31.0",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "onchange": "^7.1.0",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "rimraf": "^6.0.1",
    "rollup": "4.45.1",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss-inner-border": "^0.2.0",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-plugin-solid": "^2.11.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
